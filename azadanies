#include <iostream>
#include <fstream>
#include <string>
using namespace std;
int main()
{
    ifstream wejscie("dane_obrazki.txt");
int a,b,c;
 for(int c=0;c<20;c++){
 for(int a=0;a<20;a++){
 		string wiersz;
        	wejscie>>wiersz;
        int ilosc_jedynek=0;
        for (b=0; b<20; b++) 
        if (wiersz[b]=='1') 
		ilosc_jedynek++;}}
    int suma_jedynek;
    suma_jedynek+=ilosc_jedynek;
    cout<<suma_jedynek;

return 0;
}
